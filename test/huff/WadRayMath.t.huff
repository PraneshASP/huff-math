/* Imports */
#include "./helpers/TestHelpers.huff"
#include '../../src/WadRayMath.huff'

/* Tests */
#define test TEST_WAD_MUL() = {
    0x22b1c8c1227a0000  // [2.5e18]
    0x6f05b59d3b20000   // [0.5e18, 2.5e18]
    WAD_MUL()           // [result]
    0x1158e460913d0000  // [1.25e18,result]
    ASSERT_EQ()         // [1.25e18==result]
}

#define test TEST_WAD_MUL_EDGE_CASE() = {
    0x00                // [0]
    0xde0b6b3a7640000   // [1e18, 0]
    WAD_MUL()           // [result]
    0x00                // [0, result]
    ASSERT_EQ()         // [0==result]
}